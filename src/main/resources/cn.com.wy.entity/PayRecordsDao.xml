<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cn.com.wy.dao.PayRecordsDao" >
  <resultMap id="PayRecordsMap" type="cn.com.wy.entity.PayRecords" >
    <id column="prc_id" property="prcId"/>
    <result column="prc_date" property="prcDate"/>
    <result column="prc_remark" property="prcRemark"/>
    <result column="prc_pfid" property="pfId"/>
  </resultMap>
<insert id="addPrc" parameterType="cn.com.wy.entity.PayRecords">
  insert into t_payrecords values(null,#{prcDate},#{propertyFee.pfId},#{prcRemark})
</insert>
  <select id="findAll" resultMap="PayRecordsMap">
    select * from t_payrecords
  </select>
</mapper>